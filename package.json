{
  "name": "@platoform/pulumi-helper",
  "version": "0.0.5",
  "description": "Set of pulumi helper functions for Platoform Cloud",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@pulumi/kubernetes": "^4.23.0",
    "@pulumi/pulumi": "^3.181.0",
    "@pulumi/random": "^4.16.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@pulumi/kubernetes": ">=4.0.0",
    "@pulumi/pulumi": ">=3.0.0",
    "@pulumi/random": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plato-cloud/platoform-pulumi.git"
  },
  "keywords": [
    "pulumi",
    "platoform",
    "kubernetes",
    "ingress",
    "certbot",
    "app"
  ],
  "author": "Sergiy Kukunin <sergey.kukunin@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/plato-cloud/platoform-pulumi/issues"
  },
  "homepage": "https://github.com/plato-cloud/platoform-pulumi#readme"
}
